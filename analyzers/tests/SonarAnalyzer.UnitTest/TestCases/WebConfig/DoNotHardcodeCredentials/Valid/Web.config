    <add
      connectionString
      =
      "Server=localhost; Database=Test; User=SA; Password=Secret123" />
    <add >
    </add>
    <!--
      Commented is not supported
      <add connectionString="Server=localhost; Database=Test; User=SA; Password=Secret123"/>
    -->
